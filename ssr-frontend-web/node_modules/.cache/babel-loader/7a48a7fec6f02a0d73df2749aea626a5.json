\t\taxios.post('http://127.0.0.1:8000/users/login')\r\n\t\t\t\t} else {\r\n\t\t\t\t\tconsole.log('error submit!!');\r\n\t\t\t\t\treturn false;\r\n\t\t\t\t}\r\n\t\t\t});\r\n\t\t},\r\n\t\tresetForm(formName) {\r\n\t\t\tthis.$refs[formName].resetFields();\r\n\t\t}\r\n\t},\r\n\tname: \"LoginView\",\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\t#LoginView{\r\n\t\tmargin-top: 10%;\r\n\t\ttext-align: center;\r\n\t}\r\n</style>"],"sourceRoot":"src/components"}]}